{
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": "error",
    "import/no-anonymous-default-export": "off"
  },
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "prettier/react",
    "plugin:jest-playwright/recommended"
  ],
  "globals": {
    "globalThis": false
  }
}
